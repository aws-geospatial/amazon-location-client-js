  "keywords": [],
  "author": {
    "name": "Amazon Web Services",
    "email": "",
    "url": "https://aws.amazon.com/"
  },
  "homepage": "https://github.com/aws-geospatial/amazon-location-client-js",
  "repository": {
    "type": "git",
