    "@aws/amazon-location-utilities-auth-helper": "^1.2.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "rollup": "^4.24.0"
  }
